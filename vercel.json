{
  "version": 2,
  "builds": [
    {
      "src": "front/v-portafolio/package.json", 
      "use": "@vercel/static-build", 
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "front/v-portafolio/dist/$1"
    },
    {
      "src": "/api/(.*)", 
      "dest": "/index.js"
    }
  ]
}
