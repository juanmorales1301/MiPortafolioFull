{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/node_modules/swagger-ui-dist/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "scripts": {
    "start": "npm run start"
  }
}
